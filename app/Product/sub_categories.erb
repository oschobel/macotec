<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Produkte</h1>
    <a href="<%= Rho::RhoConfig.start_path %>" class="ui-btn-left" data-icon="home" data-direction="reverse" <%= "data-ajax='false'" if is_bb6 %>>
      Home
    </a>
  </div>
  <div data-role="content">
	<% @products.each do |key, product| %>
	<b><a href="<%= url_for :controller => :Product, :action => :product_details, :query => {:product => key} %>"><%= product %></b><br>
	<br> 
	<% end %>
  </div>

</div>